in:
  type: file
  path_prefix: {{ env.CD_FETCHED }}/module_progression_fact
  decoders:
  - {type: gzip}
  parser:
    charset: UTF-8
    newline: LF
    type: csv
    delimiter: "\t"
    quote: null
    quotes_in_quoted_fields: ACCEPT_STRAY_QUOTES_ASSUMING_NO_DELIMITERS_IN_FIELDS
    escape: '"'
    null_string: \N
    trim_if_not_quoted: false
    skip_header_lines: 0
    allow_extra_columns: false
    allow_optional_columns: false
    columns:
    - {name: module_progression_id, type: long}
    - {name: account_id, type: long}
    - {name: course_id, type: long}
    - {name: enrollment_term_id, type: long}
    - {name: module_id, type: long}
    - {name: user_id, type: long}
    - {name: wiki_id, type: long}
out:
  type: oracle
  driver_path: {{ env.EMBULK_ORACLE_DRIVER }}
  url: jdbc:oracle:thin:@{{ env.EMBULK_ORACLEH }}:{{ env.EMBULK_ORACLEHP }}/{{ env.EMBULK_ORACLEDB }}
  user: {{ env.EMBULK_ORACLEU }}
  password: {{ env.EMBULK_ORACLEP }}
  default_timezone: {{ env.EMBULK_TZ }}
  table: module_progression_fact
  mode: replace
  insert_method: normal
  column_options:
    module_progression_id: {type: 'NUMBER NOT NULL PRIMARY KEY'}
    account_id: {type: 'NUMBER NULL'}
    course_id: {type: 'NUMBER NULL'}
    enrollment_term_id: {type: 'NUMBER NULL'}
    module_id: {type: 'NUMBER NULL'}
    user_id: {type: 'NUMBER NULL'}
    wiki_id: {type: 'NUMBER NULL'}
  {% comment %} after_load: |
    CREATE INDEX "account_id" ON "module_progression_fact" ("account_id");
    CREATE INDEX "course_id" ON "module_progression_fact" ("course_id");
    CREATE INDEX "enrollment_term_id" ON "module_progression_fact" ("enrollment_term_id");
    CREATE INDEX "module_id" ON "module_progression_fact" ("module_id");
    CREATE INDEX "user_id" ON "module_progression_fact" ("user_id");
    CREATE INDEX "wiki_id" ON "module_progression_fact" ("wiki_id"); {% endcomment %}