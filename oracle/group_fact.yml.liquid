in:
  type: file
  path_prefix: {{ env.CD_FETCHED }}/group_fact
  decoders:
  - {type: gzip}
  parser:
    charset: UTF-8
    newline: LF
    type: csv
    delimiter: "\t"
    quote: null
    quotes_in_quoted_fields: ACCEPT_STRAY_QUOTES_ASSUMING_NO_DELIMITERS_IN_FIELDS
    escape: '"'
    null_string: \N
    trim_if_not_quoted: false
    skip_header_lines: 0
    allow_extra_columns: false
    allow_optional_columns: false
    columns:
    - {name: group_id, type: long}
    - {name: parent_course_id, type: long}
    - {name: parent_account_id, type: long}
    - {name: parent_course_account_id, type: long}
    - {name: enrollment_term_id, type: long}
    - {name: max_membership, type: long}
    - {name: storage_quota, type: long}
    - {name: group_category_id, type: long}
    - {name: account_id, type: long}
    - {name: wiki_id, type: long}
out:
  type: oracle
  driver_path: {{ env.EMBULK_ORACLE_DRIVER }}
  url: jdbc:oracle:thin:@{{ env.EMBULK_ORACLEH }}:{{ env.EMBULK_ORACLEHP }}/{{ env.EMBULK_ORACLEDB }}
  user: {{ env.EMBULK_ORACLEU }}
  password: {{ env.EMBULK_ORACLEP }}
  default_timezone: {{ env.EMBULK_TZ }}
  table: group_fact
  mode: replace
  insert_method: normal
  column_options:
    group_id: {type: 'NUMBER NOT NULL PRIMARY KEY'}
    parent_course_id: {type: 'NUMBER NULL'}
    parent_account_id: {type: 'NUMBER NULL'}
    parent_course_account_id: {type: 'NUMBER NULL'}
    enrollment_term_id: {type: 'NUMBER NULL'}
    max_membership: {type: 'NUMBER NULL'}
    storage_quota: {type: 'NUMBER NULL'}
    group_category_id: {type: 'NUMBER NULL'}
    account_id: {type: 'NUMBER NULL'}
    wiki_id: {type: 'NUMBER NULL'}
  {% comment %} after_load: |
    CREATE INDEX "parent_course_id" ON "group_fact" ("parent_course_id");
    CREATE INDEX "parent_account_id" ON "group_fact" ("parent_account_id");
    CREATE INDEX "parent_course_account_id" ON "group_fact" ("parent_course_account_id");
    CREATE INDEX "enrollment_term_id" ON "group_fact" ("enrollment_term_id");
    CREATE INDEX "group_category_id" ON "group_fact" ("group_category_id");
    CREATE INDEX "account_id" ON "group_fact" ("account_id");
    CREATE INDEX "wiki_id" ON "group_fact" ("wiki_id"); {% endcomment %}