in:
  type: file
  path_prefix: {{ env.CD_FETCHED }}/group_dim
  decoders:
  - {type: gzip}
  parser:
    charset: UTF-8
    newline: LF
    type: csv
    delimiter: "\t"
    quote: null
    quotes_in_quoted_fields: ACCEPT_STRAY_QUOTES_ASSUMING_NO_DELIMITERS_IN_FIELDS
    escape: '"'
    null_string: \N
    trim_if_not_quoted: false
    skip_header_lines: 0
    allow_extra_columns: false
    allow_optional_columns: false
    columns:
    - {name: id, type: long}
    - {name: canvas_id, type: long}
    - {name: name, type: string}
    - {name: description, type: string}
    - {name: created_at, type: timestamp, format: '%Y-%m-%d %H:%M:%S.%L'}
    - {name: updated_at, type: timestamp, format: '%Y-%m-%d %H:%M:%S.%L'}
    - {name: deleted_at, type: timestamp, format: '%Y-%m-%d %H:%M:%S.%L'}
    - {name: is_public, type: string}
    - {name: workflow_state, type: string}
    - {name: context_type, type: string}
    - {name: category, type: string}
    - {name: join_level, type: string}
    - {name: default_view, type: string}
    - {name: sis_source_id, type: long}
    - {name: group_category_id, type: long}
    - {name: account_id, type: long}
    - {name: wiki_id, type: long}
out:
  type: oracle
  driver_path: {{ env.EMBULK_ORACLE_DRIVER }}
  url: jdbc:oracle:thin:@{{ env.EMBULK_ORACLEH }}:{{ env.EMBULK_ORACLEHP }}/{{ env.EMBULK_ORACLEDB }}
  user: {{ env.EMBULK_ORACLEU }}
  password: {{ env.EMBULK_ORACLEP }}
  default_timezone: {{ env.EMBULK_TZ }}
  table: group_dim
  mode: replace
  insert_method: normal
  column_options:
    id: {type: 'NUMBER NOT NULL PRIMARY KEY'}
    canvas_id: {type: 'NUMBER NULL'}
    name: {type: 'VARCHAR(256) NULL'}
    description: {type: 'CLOB NULL'}
    created_at: {type: 'DATE NULL', value_type: timestamp, timestamp_format: '%Y-%m-%d %H:%M:%S.%L'}
    updated_at: {type: 'DATE NULL', value_type: timestamp, timestamp_format: '%Y-%m-%d %H:%M:%S.%L'}
    deleted_at: {type: 'DATE NULL', value_type: timestamp, timestamp_format: '%Y-%m-%d %H:%M:%S.%L'}
    is_public: {type: 'VARCHAR(256) NULL'}
    workflow_state: {type: 'VARCHAR(256) NULL'}
    context_type: {type: 'VARCHAR(256) NULL'}
    category: {type: 'VARCHAR(256) NULL'}
    join_level: {type: 'VARCHAR(256) NULL'}
    default_view: {type: 'VARCHAR(256) NULL'}
    sis_source_id: {type: 'NUMBER NULL'}
    group_category_id: {type: 'NUMBER NULL'}
    account_id: {type: 'NUMBER NULL'}
    wiki_id: {type: 'NUMBER NULL'}
  {% comment %} after_load: |
    ALTER TABLE "group_dim" ADD CONSTRAINT "CK_group_dim_is_public" CHECK ("is_public" IN ('true', 'false'));
    ALTER TABLE "group_dim" ADD CONSTRAINT "CK_group_dim_workflow_state" CHECK ("workflow_state" IN ('available', 'deleted'));
    CREATE INDEX "workflow_state" ON "group_dim" ("workflow_state");
    CREATE INDEX "sis_source_id" ON "group_dim" ("sis_source_id");
    CREATE INDEX "group_category_id" ON "group_dim" ("group_category_id");
    CREATE INDEX "account_id" ON "group_dim" ("account_id");
    CREATE INDEX "wiki_id" ON "group_dim" ("wiki_id"); {% endcomment %}