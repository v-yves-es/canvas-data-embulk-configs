in:
  type: file
  path_prefix: {{ env.CD_FETCHED }}/learning_outcome_rubric_criterion_fact
  decoders:
  - {type: gzip}
  parser:
    charset: UTF-8
    newline: LF
    type: csv
    delimiter: "\t"
    quote: null
    quotes_in_quoted_fields: ACCEPT_STRAY_QUOTES_ASSUMING_NO_DELIMITERS_IN_FIELDS
    escape: '"'
    null_string: \N
    trim_if_not_quoted: false
    skip_header_lines: 0
    allow_extra_columns: false
    allow_optional_columns: false
    columns:
    - {name: learning_outcome_rubric_criterion_id, type: long}
    - {name: learning_outcome_id, type: long}
    - {name: account_id, type: long}
    - {name: course_id, type: long}
    - {name: enrollment_term_id, type: long}
    - {name: points, type: double}
out:
  type: oracle
  driver_path: {{ env.EMBULK_ORACLE_DRIVER }}
  url: jdbc:oracle:thin:@{{ env.EMBULK_ORACLEH }}:{{ env.EMBULK_ORACLEHP }}/{{ env.EMBULK_ORACLEDB }}
  user: {{ env.EMBULK_ORACLEU }}
  password: {{ env.EMBULK_ORACLEP }}
  default_timezone: {{ env.EMBULK_TZ }}
  table: learning_outcome_rubric_criterion_fact
  mode: replace
  insert_method: normal
  column_options:
    learning_outcome_rubric_criterion_id: {type: 'NUMBER NOT NULL PRIMARY KEY'}
    learning_outcome_id: {type: 'NUMBER NULL'}
    account_id: {type: 'NUMBER NULL'}
    course_id: {type: 'NUMBER NULL'}
    enrollment_term_id: {type: 'NUMBER NULL'}
    points: {type: 'FLOAT NULL'}
  {% comment %} after_load: |
    CREATE INDEX "learning_outcome_id" ON "learning_outcome_rubric_criterion_fact" ("learning_outcome_id");
    CREATE INDEX "account_id" ON "learning_outcome_rubric_criterion_fact" ("account_id");
    CREATE INDEX "course_id" ON "learning_outcome_rubric_criterion_fact" ("course_id");
    CREATE INDEX "enrollment_term_id" ON "learning_outcome_rubric_criterion_fact" ("enrollment_term_id"); {% endcomment %}