in:
  type: file
  path_prefix: {{ env.CD_FETCHED }}/conference_participant_fact
  decoders:
  - {type: gzip}
  parser:
    charset: UTF-8
    newline: LF
    type: csv
    delimiter: "\t"
    quote: null
    quotes_in_quoted_fields: ACCEPT_STRAY_QUOTES_ASSUMING_NO_DELIMITERS_IN_FIELDS
    escape: '"'
    null_string: \N
    trim_if_not_quoted: false
    skip_header_lines: 0
    allow_extra_columns: false
    allow_optional_columns: false
    columns:
    - {name: conference_participant_id, type: long}
    - {name: group_id, type: long}
    - {name: course_id, type: long}
    - {name: account_id, type: long}
    - {name: user_id, type: long}
    - {name: conference_id, type: long}
out:
  type: oracle
  driver_path: {{ env.EMBULK_ORACLE_DRIVER }}
  url: jdbc:oracle:thin:@{{ env.EMBULK_ORACLEH }}:{{ env.EMBULK_ORACLEHP }}/{{ env.EMBULK_ORACLEDB }}
  user: {{ env.EMBULK_ORACLEU }}
  password: {{ env.EMBULK_ORACLEP }}
  default_timezone: {{ env.EMBULK_TZ }}
  table: conference_participant_fact
  mode: replace
  insert_method: normal
  column_options:
    conference_participant_id: {type: 'NUMBER NOT NULL PRIMARY KEY'}
    group_id: {type: 'NUMBER NULL'}
    course_id: {type: 'NUMBER NULL'}
    account_id: {type: 'NUMBER NULL'}
    user_id: {type: 'NUMBER NULL'}
    conference_id: {type: 'NUMBER NULL'}
  {% comment %} after_load: |
    CREATE INDEX "group_id" ON "conference_participant_fact" ("group_id");
    CREATE INDEX "course_id" ON "conference_participant_fact" ("course_id");
    CREATE INDEX "account_id" ON "conference_participant_fact" ("account_id");
    CREATE INDEX "user_id" ON "conference_participant_fact" ("user_id");
    CREATE INDEX "conference_id" ON "conference_participant_fact" ("conference_id"); {% endcomment %}