in:
  type: file
  path_prefix: {{ env.CD_FETCHED }}/quiz_submission_historical_dim
  decoders:
  - {type: gzip}
  parser:
    charset: UTF-8
    newline: LF
    type: csv
    delimiter: "\t"
    quote: null
    quotes_in_quoted_fields: ACCEPT_STRAY_QUOTES_ASSUMING_NO_DELIMITERS_IN_FIELDS
    escape: '"'
    null_string: \N
    trim_if_not_quoted: false
    skip_header_lines: 0
    allow_extra_columns: false
    allow_optional_columns: false
    columns:
    - {name: id, type: long}
    - {name: canvas_id, type: long}
    - {name: quiz_id, type: long}
    - {name: submission_id, type: long}
    - {name: user_id, type: long}
    - {name: version_number, type: long}
    - {name: submission_state, type: string}
    - {name: workflow_state, type: string}
    - {name: quiz_state_during_submission, type: string}
    - {name: submission_scoring_policy, type: string}
    - {name: submission_source, type: string}
    - {name: has_seen_results, type: string}
    - {name: temporary_user_code, type: string}
    - {name: created_at, type: timestamp, format: '%Y-%m-%d %H:%M:%S.%L'}
    - {name: updated_at, type: timestamp, format: '%Y-%m-%d %H:%M:%S.%L'}
    - {name: started_at, type: timestamp, format: '%Y-%m-%d %H:%M:%S.%L'}
    - {name: finished_at, type: timestamp, format: '%Y-%m-%d %H:%M:%S.%L'}
    - {name: due_at, type: timestamp, format: '%Y-%m-%d %H:%M:%S.%L'}
out:
  type: postgresql
  host: {{ env.EMBULK_POSTGRESH }}
  port: {{ env.EMBULK_POSTGRESHP }}
  user: {{ env.EMBULK_POSTGRESU }}
  password: {{ env.EMBULK_POSTGRESP }}
  database: {{ env.EMBULK_POSTGRESDB }}
  default_timezone: {{ env.EMBULK_TZ }}
  table: quiz_submission_historical_dim
  mode: replace
  insert_method: native
  column_options:
    id: {type: 'BIGINT NOT NULL PRIMARY KEY'}
    canvas_id: {type: 'INT NULL'}
    quiz_id: {type: 'BIGINT NULL'}
    submission_id: {type: 'BIGINT NULL'}
    user_id: {type: 'BIGINT NULL'}
    version_number: {type: 'INT NULL'}
    submission_state: {type: 'VARCHAR(256) NULL'}
    workflow_state: {type: 'VARCHAR(256) NULL'}
    quiz_state_during_submission: {type: 'VARCHAR(256) NULL'}
    submission_scoring_policy: {type: 'VARCHAR(256) NULL'}
    submission_source: {type: 'VARCHAR(256) NULL'}
    has_seen_results: {type: 'VARCHAR(256) NULL'}
    temporary_user_code: {type: 'VARCHAR(256) NULL'}
    created_at: {value_type: timestamp, timestamp_format: '%Y-%m-%d %H:%M:%S.%L'}
    updated_at: {value_type: timestamp, timestamp_format: '%Y-%m-%d %H:%M:%S.%L'}
    started_at: {value_type: timestamp, timestamp_format: '%Y-%m-%d %H:%M:%S.%L'}
    finished_at: {value_type: timestamp, timestamp_format: '%Y-%m-%d %H:%M:%S.%L'}
    due_at: {value_type: timestamp, timestamp_format: '%Y-%m-%d %H:%M:%S.%L'}
  after_load: |
    ALTER TABLE "quiz_submission_historical_dim" ADD CONSTRAINT "CK_quiz_submission_historical_dim_submission_state" CHECK ("submission_state" IN ('current_submission', 'previous_submission'));
    ALTER TABLE "quiz_submission_historical_dim" ADD CONSTRAINT "CK_quiz_submission_historical_dim_workflow_state" CHECK ("workflow_state" IN ('complete', 'pending_review', 'preview', 'settings_only', 'untaken'));
    ALTER TABLE "quiz_submission_historical_dim" ADD CONSTRAINT "CK_quiz_submission_historical_dim_submission_scoring_policy" CHECK ("submission_scoring_policy" IN ('keep_average', 'keep_highest', 'keep_latest', 'manually_overridden'));
    ALTER TABLE "quiz_submission_historical_dim" ADD CONSTRAINT "CK_quiz_submission_historical_dim_submission_source" CHECK ("submission_source" IN ('student', 'test_preview'));
    CREATE INDEX "quiz_submission_historical_dim_idx_quiz_id" ON "quiz_submission_historical_dim" ("quiz_id");
    CREATE INDEX "quiz_submission_historical_dim_idx_submission_id" ON "quiz_submission_historical_dim" ("submission_id");
    CREATE INDEX "quiz_submission_historical_dim_idx_user_id" ON "quiz_submission_historical_dim" ("user_id");
    CREATE INDEX "quiz_submission_historical_dim_idx_workflow_state" ON "quiz_submission_historical_dim" ("workflow_state");