in:
  type: file
  path_prefix: {{ env.CD_FETCHED }}/learning_outcome_result_dim
  decoders:
  - {type: gzip}
  parser:
    charset: UTF-8
    newline: LF
    type: csv
    delimiter: "\t"
    quote: null
    quotes_in_quoted_fields: ACCEPT_STRAY_QUOTES_ASSUMING_NO_DELIMITERS_IN_FIELDS
    escape: '"'
    null_string: \N
    trim_if_not_quoted: false
    skip_header_lines: 0
    allow_extra_columns: false
    allow_optional_columns: false
    columns:
    - {name: id, type: long}
    - {name: canvas_id, type: long}
    - {name: account_id, type: long}
    - {name: course_id, type: long}
    - {name: assignment_id, type: long}
    - {name: quiz_id, type: long}
    - {name: learning_outcome_id, type: long}
    - {name: user_id, type: long}
    - {name: created_at, type: timestamp, format: '%Y-%m-%d %H:%M:%S.%L'}
    - {name: updated_at, type: timestamp, format: '%Y-%m-%d %H:%M:%S.%L'}
    - {name: assessed_at, type: timestamp, format: '%Y-%m-%d %H:%M:%S.%L'}
    - {name: submitted_at, type: timestamp, format: '%Y-%m-%d %H:%M:%S.%L'}
    - {name: hide_points, type: string}
    - {name: hidden, type: string}
out:
  type: postgresql
  host: {{ env.EMBULK_POSTGRESH }}
  port: {{ env.EMBULK_POSTGRESHP }}
  user: {{ env.EMBULK_POSTGRESU }}
  password: {{ env.EMBULK_POSTGRESP }}
  database: {{ env.EMBULK_POSTGRESDB }}
  default_timezone: {{ env.EMBULK_TZ }}
  table: learning_outcome_result_dim
  mode: replace
  insert_method: native
  column_options:
    id: {type: 'BIGINT NOT NULL PRIMARY KEY'}
    canvas_id: {type: 'INT NULL'}
    account_id: {type: 'BIGINT NULL'}
    course_id: {type: 'BIGINT NULL'}
    assignment_id: {type: 'BIGINT NULL'}
    quiz_id: {type: 'BIGINT NULL'}
    learning_outcome_id: {type: 'BIGINT NULL'}
    user_id: {type: 'BIGINT NULL'}
    created_at: {value_type: timestamp, timestamp_format: '%Y-%m-%d %H:%M:%S.%L'}
    updated_at: {value_type: timestamp, timestamp_format: '%Y-%m-%d %H:%M:%S.%L'}
    assessed_at: {value_type: timestamp, timestamp_format: '%Y-%m-%d %H:%M:%S.%L'}
    submitted_at: {value_type: timestamp, timestamp_format: '%Y-%m-%d %H:%M:%S.%L'}
    hide_points: {type: 'VARCHAR(256) NULL'}
    hidden: {type: 'VARCHAR(256) NULL'}
  after_load: |
    ALTER TABLE "learning_outcome_result_dim" ADD CONSTRAINT "CK_learning_outcome_result_dim_hide_points" CHECK ("hide_points" IN ('true', 'false'));
    ALTER TABLE "learning_outcome_result_dim" ADD CONSTRAINT "CK_learning_outcome_result_dim_hidden" CHECK ("hidden" IN ('true', 'false'));
    CREATE INDEX "learning_outcome_result_dim_idx_account_id" ON "learning_outcome_result_dim" ("account_id");
    CREATE INDEX "learning_outcome_result_dim_idx_course_id" ON "learning_outcome_result_dim" ("course_id");
    CREATE INDEX "learning_outcome_result_dim_idx_assignment_id" ON "learning_outcome_result_dim" ("assignment_id");
    CREATE INDEX "learning_outcome_result_dim_idx_quiz_id" ON "learning_outcome_result_dim" ("quiz_id");
    CREATE INDEX "learning_outcome_result_dim_idx_learning_outcome_id" ON "learning_outcome_result_dim" ("learning_outcome_id");
    CREATE INDEX "learning_outcome_result_dim_idx_user_id" ON "learning_outcome_result_dim" ("user_id");