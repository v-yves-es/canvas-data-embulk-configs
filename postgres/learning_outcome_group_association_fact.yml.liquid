in:
  type: file
  path_prefix: {{ env.CD_FETCHED }}/learning_outcome_group_association_fact
  decoders:
  - {type: gzip}
  parser:
    charset: UTF-8
    newline: LF
    type: csv
    delimiter: "\t"
    quote: null
    quotes_in_quoted_fields: ACCEPT_STRAY_QUOTES_ASSUMING_NO_DELIMITERS_IN_FIELDS
    escape: '"'
    null_string: \N
    trim_if_not_quoted: false
    skip_header_lines: 0
    allow_extra_columns: false
    allow_optional_columns: false
    columns:
    - {name: learning_outcome_id, type: long}
    - {name: learning_outcome_group_id, type: long}
    - {name: account_id, type: long}
    - {name: course_id, type: long}
    - {name: enrollment_term_id, type: long}
out:
  type: postgresql
  host: {{ env.EMBULK_POSTGRESH }}
  port: {{ env.EMBULK_POSTGRESHP }}
  user: {{ env.EMBULK_POSTGRESU }}
  password: {{ env.EMBULK_POSTGRESP }}
  database: {{ env.EMBULK_POSTGRESDB }}
  default_timezone: {{ env.EMBULK_TZ }}
  table: learning_outcome_group_association_fact
  mode: replace
  insert_method: native
  column_options:
    learning_outcome_id: {type: 'BIGINT NOT NULL'} # no primary key
    learning_outcome_group_id: {type: 'BIGINT NULL'}
    account_id: {type: 'BIGINT NULL'}
    course_id: {type: 'BIGINT NULL'}
    enrollment_term_id: {type: 'BIGINT NULL'}
  after_load: |
    CREATE INDEX "learning_outcome_group_association_fact_idx_learning_outcome_id" ON "learning_outcome_group_association_fact" ("learning_outcome_id");
    CREATE INDEX "learning_outcome_group_association_fact_idx_learning_outcome_group_id" ON "learning_outcome_group_association_fact" ("learning_outcome_group_id");
    CREATE INDEX "learning_outcome_group_association_fact_idx_account_id" ON "learning_outcome_group_association_fact" ("account_id");
    CREATE INDEX "learning_outcome_group_association_fact_idx_course_id" ON "learning_outcome_group_association_fact" ("course_id");
    CREATE INDEX "learning_outcome_group_association_fact_idx_enrollment_term_id" ON "learning_outcome_group_association_fact" ("enrollment_term_id");