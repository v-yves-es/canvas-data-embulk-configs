in:
  type: file
  path_prefix: {{ env.CD_FETCHED }}/conference_participant_dim
  decoders:
  - {type: gzip}
  parser:
    charset: UTF-8
    newline: LF
    type: csv
    delimiter: "\t"
    quote: null
    quotes_in_quoted_fields: ACCEPT_STRAY_QUOTES_ASSUMING_NO_DELIMITERS_IN_FIELDS
    escape: '"'
    null_string: \N
    trim_if_not_quoted: false
    skip_header_lines: 0
    allow_extra_columns: false
    allow_optional_columns: false
    columns:
    - {name: id, type: long}
    - {name: user_id, type: long}
    - {name: conference_id, type: long}
    - {name: participation_type, type: string}
    - {name: created_at, type: timestamp, format: '%Y-%m-%d %H:%M:%S.%L'}
    - {name: updated_at, type: timestamp, format: '%Y-%m-%d %H:%M:%S.%L'}
out:
  type: mysql
  host: {{ env.EMBULK_MYSQLH }}
  port: {{ env.EMBULK_MYSQLHP }}
  user: {{ env.EMBULK_MYSQLU }}
  password: {{ env.EMBULK_MYSQLP }}
  database: {{ env.EMBULK_MYSQLDB }}
  default_timezone: {{ env.EMBULK_TZ }}
  table: conference_participant_dim
  mode: replace
  insert_method: native
  column_options:
    id: {type: 'BIGINT NOT NULL PRIMARY KEY'}
    user_id: {type: 'BIGINT NULL'}
    conference_id: {type: 'BIGINT NULL'}
    participation_type: {type: 'VARCHAR(256) NULL'}
    created_at: {type: 'DATETIME NULL', value_type: timestamp, timestamp_format: '%Y-%m-%d %H:%M:%S.%L'}
    updated_at: {type: 'DATETIME NULL', value_type: timestamp, timestamp_format: '%Y-%m-%d %H:%M:%S.%L'}
  after_load: |
    ALTER TABLE `conference_participant_dim`
      ADD INDEX `user_id` (`user_id`),
      ADD INDEX `conference_id` (`conference_id`);