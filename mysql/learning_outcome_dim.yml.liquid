in:
  type: file
  path_prefix: {{ env.CD_FETCHED }}/learning_outcome_dim
  decoders:
  - {type: gzip}
  parser:
    charset: UTF-8
    newline: LF
    type: csv
    delimiter: "\t"
    quote: null
    quotes_in_quoted_fields: ACCEPT_STRAY_QUOTES_ASSUMING_NO_DELIMITERS_IN_FIELDS
    escape: '"'
    null_string: \N
    trim_if_not_quoted: false
    skip_header_lines: 0
    allow_extra_columns: false
    allow_optional_columns: false
    columns:
    - {name: id, type: long}
    - {name: canvas_id, type: long}
    - {name: account_id, type: long}
    - {name: course_id, type: long}
    - {name: short_description, type: string}
    - {name: description, type: string}
    - {name: workflow_state, type: string}
    - {name: created_at, type: timestamp, format: '%Y-%m-%d %H:%M:%S.%L'}
    - {name: updated_at, type: timestamp, format: '%Y-%m-%d %H:%M:%S.%L'}
    - {name: vendor_guid, type: string}
    - {name: display_name, type: string}
    - {name: calculation_method, type: string}
    - {name: calculation_int, type: long}
    - {name: outcome_import_id, type: long}
out:
  type: mysql
  host: {{ env.EMBULK_MYSQLH }}
  port: {{ env.EMBULK_MYSQLHP }}
  user: {{ env.EMBULK_MYSQLU }}
  password: {{ env.EMBULK_MYSQLP }}
  database: {{ env.EMBULK_MYSQLDB }}
  default_timezone: {{ env.EMBULK_TZ }}
  table: learning_outcome_dim
  mode: replace
  insert_method: native
  column_options:
    id: {type: 'BIGINT NOT NULL PRIMARY KEY'}
    canvas_id: {type: 'INT NULL'}
    account_id: {type: 'BIGINT NULL'}
    course_id: {type: 'BIGINT NULL'}
    short_description: {type: 'VARCHAR(256) NULL'}
    description: {type: 'LONGTEXT NULL'}
    workflow_state: {type: 'VARCHAR(256) NULL'}
    created_at: {type: 'DATETIME NULL', value_type: timestamp, timestamp_format: '%Y-%m-%d %H:%M:%S.%L'}
    updated_at: {type: 'DATETIME NULL', value_type: timestamp, timestamp_format: '%Y-%m-%d %H:%M:%S.%L'}
    vendor_guid: {type: 'VARCHAR(256) NULL'}
    display_name: {type: 'VARCHAR(256) NULL'}
    calculation_method: {type: 'VARCHAR(256) NULL'}
    calculation_int: {type: 'INT NULL'}
    outcome_import_id: {type: 'BIGINT NULL'}
  after_load: |
    ALTER TABLE `learning_outcome_dim`
      CHANGE `workflow_state` `workflow_state` ENUM('active', 'deleted'),
      ADD INDEX `account_id` (`account_id`),
      ADD INDEX `course_id` (`course_id`),
      ADD INDEX `workflow_state` (`workflow_state`),
      ADD INDEX `outcome_import_id` (`outcome_import_id`);