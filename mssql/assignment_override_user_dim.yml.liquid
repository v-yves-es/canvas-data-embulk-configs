in:
  type: file
  path_prefix: {{ env.CD_FETCHED }}/assignment_override_user_dim
  decoders:
  - {type: gzip}
  parser:
    charset: UTF-8
    newline: LF
    type: csv
    delimiter: "\t"
    quote: null
    quotes_in_quoted_fields: ACCEPT_STRAY_QUOTES_ASSUMING_NO_DELIMITERS_IN_FIELDS
    escape: '"'
    null_string: \N
    trim_if_not_quoted: false
    skip_header_lines: 0
    allow_extra_columns: false
    allow_optional_columns: false
    columns:
    - {name: id, type: long}
    - {name: canvas_id, type: long}
    - {name: assignment_id, type: long}
    - {name: assignment_override_id, type: long}
    - {name: quiz_id, type: long}
    - {name: user_id, type: long}
    - {name: created_at, type: timestamp, format: '%Y-%m-%d %H:%M:%S.%L'}
    - {name: updated_at, type: timestamp, format: '%Y-%m-%d %H:%M:%S.%L'}
out:
  type: sqlserver
  driver_path: {{ env.EMBULK_MSSQL_DRIVER }}
  native_driver: {{ env.EMBULK_NATIVE_DRIVER }}
  host: {{ env.EMBULK_MSSQLH }}
  port: {{ env.EMBULK_MSSQLHP }}
  user: {{ env.EMBULK_MSSQLU }}
  password: {{ env.EMBULK_MSSQLP }}
  database: {{ env.EMBULK_MSSQLDB }}
  default_timezone: {{ env.EMBULK_TZ }}
  table: assignment_override_user_dim
  mode: replace
  insert_method: native
  column_options:
    id: {type: 'BIGINT NOT NULL PRIMARY KEY'}
    canvas_id: {type: 'INT NULL'}
    assignment_id: {type: 'BIGINT NULL'}
    assignment_override_id: {type: 'BIGINT NULL'}
    quiz_id: {type: 'BIGINT NULL'}
    user_id: {type: 'BIGINT NULL'}
    created_at: {value_type: timestamp, timestamp_format: '%Y-%m-%d %H:%M:%S.%L'}
    updated_at: {value_type: timestamp, timestamp_format: '%Y-%m-%d %H:%M:%S.%L'}
  after_load: |
    CREATE INDEX [assignment_id] ON [dbo].[assignment_override_user_dim] ([assignment_id] ASC);
    CREATE INDEX [assignment_override_id] ON [dbo].[assignment_override_user_dim] ([assignment_override_id] ASC);
    CREATE INDEX [quiz_id] ON [dbo].[assignment_override_user_dim] ([quiz_id] ASC);
    CREATE INDEX [user_id] ON [dbo].[assignment_override_user_dim] ([user_id] ASC);