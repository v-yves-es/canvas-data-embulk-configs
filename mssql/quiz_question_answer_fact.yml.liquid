in:
  type: file
  path_prefix: {{ env.CD_FETCHED }}/quiz_question_answer_fact
  decoders:
  - {type: gzip}
  parser:
    charset: UTF-8
    newline: LF
    type: csv
    delimiter: "\t"
    quote: null
    quotes_in_quoted_fields: ACCEPT_STRAY_QUOTES_ASSUMING_NO_DELIMITERS_IN_FIELDS
    escape: '"'
    null_string: \N
    trim_if_not_quoted: false
    skip_header_lines: 0
    allow_extra_columns: false
    allow_optional_columns: false
    columns:
    - {name: quiz_question_answer_id, type: long}
    - {name: quiz_question_id, type: long}
    - {name: quiz_question_group_id, type: long}
    - {name: quiz_id, type: long}
    - {name: assessment_question_id, type: long}
    - {name: course_id, type: long}
    - {name: assignment_id, type: long}
    - {name: course_account_id, type: long}
    - {name: enrollment_term_id, type: long}
    - {name: weight, type: double}
    - {name: exact, type: double}
    - {name: margin, type: double}
    - {name: starting_range, type: double}
    - {name: ending_range, type: double}
out:
  type: sqlserver
  driver_path: {{ env.EMBULK_MSSQL_DRIVER }}
  native_driver: {{ env.EMBULK_NATIVE_DRIVER }}
  host: {{ env.EMBULK_MSSQLH }}
  port: {{ env.EMBULK_MSSQLHP }}
  user: {{ env.EMBULK_MSSQLU }}
  password: {{ env.EMBULK_MSSQLP }}
  database: {{ env.EMBULK_MSSQLDB }}
  default_timezone: {{ env.EMBULK_TZ }}
  table: quiz_question_answer_fact
  mode: replace
  insert_method: native
  column_options:
    quiz_question_answer_id: {type: 'BIGINT NOT NULL PRIMARY KEY'}
    quiz_question_id: {type: 'BIGINT NULL'}
    quiz_question_group_id: {type: 'BIGINT NULL'}
    quiz_id: {type: 'BIGINT NULL'}
    assessment_question_id: {type: 'BIGINT NULL'}
    course_id: {type: 'BIGINT NULL'}
    assignment_id: {type: 'BIGINT NULL'}
    course_account_id: {type: 'BIGINT NULL'}
    enrollment_term_id: {type: 'BIGINT NULL'}
    weight: {type: 'FLOAT NULL'}
    exact: {type: 'FLOAT NULL'}
    margin: {type: 'FLOAT NULL'}
    starting_range: {type: 'FLOAT NULL'}
    ending_range: {type: 'FLOAT NULL'}
  after_load: |
    CREATE INDEX [quiz_question_id] ON [dbo].[quiz_question_answer_fact] ([quiz_question_id] ASC);
    CREATE INDEX [quiz_question_group_id] ON [dbo].[quiz_question_answer_fact] ([quiz_question_group_id] ASC);
    CREATE INDEX [quiz_id] ON [dbo].[quiz_question_answer_fact] ([quiz_id] ASC);
    CREATE INDEX [assessment_question_id] ON [dbo].[quiz_question_answer_fact] ([assessment_question_id] ASC);
    CREATE INDEX [course_id] ON [dbo].[quiz_question_answer_fact] ([course_id] ASC);
    CREATE INDEX [assignment_id] ON [dbo].[quiz_question_answer_fact] ([assignment_id] ASC);
    CREATE INDEX [course_account_id] ON [dbo].[quiz_question_answer_fact] ([course_account_id] ASC);
    CREATE INDEX [enrollment_term_id] ON [dbo].[quiz_question_answer_fact] ([enrollment_term_id] ASC);