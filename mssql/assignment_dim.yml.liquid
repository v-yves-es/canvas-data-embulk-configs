in:
  type: file
  path_prefix: {{ env.CD_FETCHED }}/assignment_dim
  decoders:
  - {type: gzip}
  parser:
    charset: UTF-8
    newline: LF
    type: csv
    delimiter: "\t"
    quote: null
    quotes_in_quoted_fields: ACCEPT_STRAY_QUOTES_ASSUMING_NO_DELIMITERS_IN_FIELDS
    null_string: \N
    trim_if_not_quoted: false
    skip_header_lines: 0
    allow_extra_columns: false
    allow_optional_columns: false
    columns:
    - {name: id, type: long}
    - {name: canvas_id, type: long}
    - {name: course_id, type: long}
    - {name: title, type: string}
    - {name: description, type: string}
    - {name: due_at, type: timestamp, format: '%Y-%m-%d %H:%M:%S.%L'}
    - {name: unlock_at, type: timestamp, format: '%Y-%m-%d %H:%M:%S.%L'}
    - {name: lock_at, type: timestamp, format: '%Y-%m-%d %H:%M:%S.%L'}
    - {name: points_possible, type: double}
    - {name: grading_type, type: string}
    - {name: submission_types, type: string}
    - {name: workflow_state, type: string}
    - {name: created_at, type: timestamp, format: '%Y-%m-%d %H:%M:%S.%L'}
    - {name: updated_at, type: timestamp, format: '%Y-%m-%d %H:%M:%S.%L'}
    - {name: peer_review_count, type: long}
    - {name: peer_reviews_due_at, type: timestamp, format: '%Y-%m-%d %H:%M:%S.%L'}
    - {name: peer_reviews_assigned, type: string}
    - {name: peer_reviews, type: string}
    - {name: automatic_peer_reviews, type: string}
    - {name: all_day, type: string}
    - {name: all_day_date, type: timestamp, format: '%Y-%m-%d'}
    - {name: could_be_locked, type: string}
    - {name: grade_group_students_individually, type: string}
    - {name: anonymous_peer_reviews, type: string}
    - {name: muted, type: string}
    - {name: assignment_group_id, type: long}
    - {name: position, type: long}
    - {name: visibility, type: string}
    - {name: external_tool_id, type: long}
out:
  type: sqlserver
  driver_path: {{ env.EMBULK_MSSQL_DRIVER }}
  native_driver: {{ env.EMBULK_NATIVE_DRIVER }}
  host: {{ env.EMBULK_MSSQLH }}
  port: {{ env.EMBULK_MSSQLHP }}
  user: {{ env.EMBULK_MSSQLU }}
  password: {{ env.EMBULK_MSSQLP }}
  database: {{ env.EMBULK_MSSQLDB }}
  default_timezone: {{ env.EMBULK_TZ }}
  table: assignment_dim
  mode: replace
  insert_method: native
  column_options:
    id: {type: 'BIGINT NOT NULL PRIMARY KEY'}
    canvas_id: {type: 'BIGINT NULL'}
    course_id: {type: 'BIGINT NULL'}
    title: {type: 'NVARCHAR(256) NULL'}
    description: {type: 'NVARCHAR(MAX) NULL'}
    due_at: {value_type: timestamp, timestamp_format: '%Y-%m-%d %H:%M:%S.%L'}
    unlock_at: {value_type: timestamp, timestamp_format: '%Y-%m-%d %H:%M:%S.%L'}
    lock_at: {value_type: timestamp, timestamp_format: '%Y-%m-%d %H:%M:%S.%L'}
    points_possible: {type: 'FLOAT NULL'}
    grading_type: {type: 'VARCHAR(256) NULL'}
    submission_types: {type: 'VARCHAR(256) NULL'}
    workflow_state: {type: 'VARCHAR(256) NULL'}
    created_at: {value_type: timestamp, timestamp_format: '%Y-%m-%d %H:%M:%S.%L'}
    updated_at: {value_type: timestamp, timestamp_format: '%Y-%m-%d %H:%M:%S.%L'}
    peer_review_count: {type: 'INT NULL'}
    peer_reviews_due_at: {value_type: timestamp, timestamp_format: '%Y-%m-%d %H:%M:%S.%L'}
    peer_reviews_assigned: {type: 'VARCHAR(256) NULL'}
    peer_reviews: {type: 'VARCHAR(256) NULL'}
    automatic_peer_reviews: {type: 'VARCHAR(256) NULL'}
    all_day: {type: 'VARCHAR(256) NULL'}
    all_day_date: {type: 'DATE NULL'}
    could_be_locked: {type: 'VARCHAR(256) NULL'}
    grade_group_students_individually: {type: 'VARCHAR(256) NULL'}
    anonymous_peer_reviews: {type: 'VARCHAR(256) NULL'}
    # deprecated # muted: {type: 'VARCHAR(256) NULL'}
    assignment_group_id: {type: 'BIGINT NULL'}
    position: {type: 'INT NULL'}
    visibility: {type: 'VARCHAR(256) NULL'}
    external_tool_id: {type: 'BIGINT NULL'}
  after_load: |
    ALTER TABLE [dbo].[assignment_dim] ADD CONSTRAINT [CK_assignment_dim_workflow_state] CHECK ([workflow_state] IN ('deleted', 'duplicating', 'fail_to_import', 'failed_to_duplicate', 'failed_to_import', 'failed_to_migrate', 'published', 'unpublished'));
    ALTER TABLE [dbo].[assignment_dim] ADD CONSTRAINT [CK_assignment_dim_peer_reviews_assigned] CHECK([peer_reviews_assigned] IN ('true', 'false'));
    ALTER TABLE [dbo].[assignment_dim] ADD CONSTRAINT [CK_assignment_dim_peer_reviews] CHECK([peer_reviews] IN ('true', 'false'));
    ALTER TABLE [dbo].[assignment_dim] ADD CONSTRAINT [CK_assignment_dim_automatic_peer_reviews] CHECK([automatic_peer_reviews] IN ('true', 'false'));
    ALTER TABLE [dbo].[assignment_dim] ADD CONSTRAINT [CK_assignment_dim_all_day] CHECK([all_day] IN ('true', 'false'));
    ALTER TABLE [dbo].[assignment_dim] ADD CONSTRAINT [CK_assignment_dim_could_be_locked] CHECK([could_be_locked] IN ('true', 'false'));
    ALTER TABLE [dbo].[assignment_dim] ADD CONSTRAINT [CK_assignment_dim_grade_group_students_individually] CHECK([grade_group_students_individually] IN ('true', 'false'));
    ALTER TABLE [dbo].[assignment_dim] ADD CONSTRAINT [CK_assignment_dim_anonymous_peer_reviews] CHECK([anonymous_peer_reviews] IN ('true', 'false'));
    ALTER TABLE [dbo].[assignment_dim] ADD CONSTRAINT [CK_assignment_dim_visibility] CHECK([visibility] IN ('everyone', 'only_visible_to_overrides'));
    CREATE INDEX [course_id] ON [dbo].[assignment_dim] ([course_id] ASC);
    CREATE INDEX [workflow_state] ON [dbo].[assignment_dim] ([workflow_state] ASC);
    CREATE INDEX [assignment_group_id] ON [dbo].[assignment_dim] ([assignment_group_id] ASC);
    CREATE INDEX [external_tool_id] ON [dbo].[assignment_dim] ([external_tool_id] ASC);