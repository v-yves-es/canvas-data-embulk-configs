in:
  type: file
  path_prefix: {{ env.CD_FETCHED }}/learning_outcome_result_fact
  decoders:
  - {type: gzip}
  parser:
    charset: UTF-8
    newline: LF
    type: csv
    delimiter: "\t"
    quote: null
    quotes_in_quoted_fields: ACCEPT_STRAY_QUOTES_ASSUMING_NO_DELIMITERS_IN_FIELDS
    escape: '"'
    null_string: \N
    trim_if_not_quoted: false
    skip_header_lines: 0
    allow_extra_columns: false
    allow_optional_columns: false
    columns:
    - {name: learning_outcome_result_id, type: long}
    - {name: account_id, type: long}
    - {name: course_id, type: long}
    - {name: enrollment_term_id, type: long}
    - {name: assignment_id, type: long}
    - {name: quiz_id, type: long}
    - {name: learning_outcome_id, type: long}
    - {name: user_id, type: long}
    - {name: mastery, type: string}
    - {name: score, type: double}
    - {name: attempts, type: long}
    - {name: possible, type: double}
    - {name: original_score, type: double}
    - {name: original_possible, type: double}
    - {name: original_mastery, type: string}
    - {name: percent, type: double}
out:
  type: sqlserver
  driver_path: {{ env.EMBULK_MSSQL_DRIVER }}
  native_driver: {{ env.EMBULK_NATIVE_DRIVER }}
  host: {{ env.EMBULK_MSSQLH }}
  port: {{ env.EMBULK_MSSQLHP }}
  user: {{ env.EMBULK_MSSQLU }}
  password: {{ env.EMBULK_MSSQLP }}
  database: {{ env.EMBULK_MSSQLDB }}
  default_timezone: {{ env.EMBULK_TZ }}
  table: learning_outcome_result_fact
  mode: replace
  insert_method: native
  column_options:
    learning_outcome_result_id: {type: 'BIGINT NOT NULL PRIMARY KEY'}
    account_id: {type: 'BIGINT NULL'}
    course_id: {type: 'BIGINT NULL'}
    enrollment_term_id: {type: 'BIGINT NULL'}
    assignment_id: {type: 'BIGINT NULL'}
    quiz_id: {type: 'BIGINT NULL'}
    learning_outcome_id: {type: 'BIGINT NULL'}
    user_id: {type: 'BIGINT NULL'}
    mastery: {type: 'VARCHAR(256) NULL'}
    score: {type: 'FLOAT NULL'}
    attempts: {type: 'INT NULL'}
    possible: {type: 'FLOAT NULL'}
    original_score: {type: 'FLOAT NULL'}
    original_possible: {type: 'FLOAT NULL'}
    original_mastery: {type: 'VARCHAR(256) NULL'}
    percent: {type: 'FLOAT NULL'}
  after_load: |
    ALTER TABLE [dbo].[learning_outcome_result_fact] ADD CONSTRAINT [CK_learning_outcome_result_fact_mastery] CHECK([mastery] IN ('true', 'false'));
    ALTER TABLE [dbo].[learning_outcome_result_fact] ADD CONSTRAINT [CK_learning_outcome_result_fact_original_mastery] CHECK([original_mastery] IN ('true', 'false'));
    CREATE INDEX [account_id] ON [dbo].[learning_outcome_result_fact] ([account_id] ASC);
    CREATE INDEX [course_id] ON [dbo].[learning_outcome_result_fact] ([course_id] ASC);
    CREATE INDEX [enrollment_term_id] ON [dbo].[learning_outcome_result_fact] ([enrollment_term_id] ASC);
    CREATE INDEX [assignment_id] ON [dbo].[learning_outcome_result_fact] ([assignment_id] ASC);
    CREATE INDEX [quiz_id] ON [dbo].[learning_outcome_result_fact] ([quiz_id] ASC);
    CREATE INDEX [learning_outcome_id] ON [dbo].[learning_outcome_result_fact] ([learning_outcome_id] ASC);
    CREATE INDEX [user_id] ON [dbo].[learning_outcome_result_fact] ([user_id] ASC);