in:
  type: file
  path_prefix: {{ env.CD_FETCHED }}/external_tool_activation_dim
  decoders:
  - {type: gzip}
  parser:
    charset: UTF-8
    newline: LF
    type: csv
    delimiter: "\t"
    quote: null
    quotes_in_quoted_fields: ACCEPT_STRAY_QUOTES_ASSUMING_NO_DELIMITERS_IN_FIELDS
    escape: '"'
    null_string: \N
    trim_if_not_quoted: false
    skip_header_lines: 0
    allow_extra_columns: false
    allow_optional_columns: false
    columns:
    - {name: id, type: long}
    - {name: canvas_id, type: long}
    - {name: course_id, type: long}
    - {name: account_id, type: long}
    - {name: activation_target_type, type: string}
    - {name: url, type: string}
    - {name: name, type: string}
    - {name: description, type: string}
    - {name: workflow_state, type: string}
    - {name: privacy_level, type: string}
    - {name: created_at, type: timestamp, format: '%Y-%m-%d %H:%M:%S.%L'}
    - {name: updated_at, type: timestamp, format: '%Y-%m-%d %H:%M:%S.%L'}
    - {name: tool_id, type: string}
    - {name: selectable_all, type: string}
out:
  type: sqlserver
  driver_path: {{ env.EMBULK_MSSQL_DRIVER }}
  native_driver: {{ env.EMBULK_NATIVE_DRIVER }}
  host: {{ env.EMBULK_MSSQLH }}
  port: {{ env.EMBULK_MSSQLHP }}
  user: {{ env.EMBULK_MSSQLU }}
  password: {{ env.EMBULK_MSSQLP }}
  database: {{ env.EMBULK_MSSQLDB }}
  default_timezone: {{ env.EMBULK_TZ }}
  table: external_tool_activation_dim
  mode: replace
  insert_method: native
  column_options:
    id: {type: 'BIGINT NOT NULL PRIMARY KEY'}
    canvas_id: {type: 'INT NULL'}
    course_id: {type: 'BIGINT NULL'}
    account_id: {type: 'BIGINT NULL'}
    activation_target_type: {type: 'VARCHAR(256) NULL'}
    url: {type: 'NVARCHAR(MAX) NULL'}
    name: {type: 'NVARCHAR(256) NULL'}
    description: {type: 'VARCHAR(256) NULL'}
    workflow_state: {type: 'VARCHAR(256) NULL'}
    privacy_level: {type: 'VARCHAR(256) NULL'}
    created_at: {value_type: timestamp, timestamp_format: '%Y-%m-%d %H:%M:%S.%L'}
    updated_at: {value_type: timestamp, timestamp_format: '%Y-%m-%d %H:%M:%S.%L'}
    tool_id: {type: 'VARCHAR(256) NULL'}
    selectable_all: {type: 'VARCHAR(256) NULL'}
  after_load: |
    ALTER TABLE [dbo].[external_tool_activation_dim] ADD CONSTRAINT [CK_external_tool_activation_dim_workflow_state] CHECK([workflow_state] IN ('deleted', 'active'));
    ALTER TABLE [dbo].[external_tool_activation_dim] ADD CONSTRAINT [CK_external_tool_activation_dim_selectable_all] CHECK([selectable_all] IN ('true', 'false'));
    CREATE INDEX [course_id] ON [dbo].[external_tool_activation_dim] ([course_id] ASC);
    CREATE INDEX [account_id] ON [dbo].[external_tool_activation_dim] ([account_id] ASC);
    CREATE INDEX [workflow_state] ON [dbo].[external_tool_activation_dim] ([workflow_state] ASC);
    CREATE INDEX [tool_id] ON [dbo].[external_tool_activation_dim] ([tool_id] ASC);